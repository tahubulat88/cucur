<script nonce="MTI5NjUxNjU4MCwyMDYzNzcxMDYx">timeLeft = 3;

  function countdown() {
    timeLeft--;
    document.getElementById("seconds").innerHTML = String(timeLeft);
    if (timeLeft > 0) {
      setTimeout(countdown, 1000);
    }
  };

  setTimeout(countdown, 1000);
</script>
