    $('.togglenearby').hide();
    $('.togglenearbybtn').click(function() {
      // $('.togglenearby').toggle('slow');
      $(".togglenearby").slideToggle("slow", function() {});
      // $('#allevent').empty();
      var id = 0;
      $.ajax({
        type: 'POST',
        url: "https://www.tixtm.com/getnearbyevents/" + id,
        beforeSend: function() {
          // Show image container
          function isEmpty(el) {
            return !$.trim(el.html())
          }
          if (isEmpty($('#allevent'))) {
            // if ($('#allevent').is(':empty')){
            $("#loader").show();
          }
        },
        // dataType: "html",
        success: function(resultData) {
          // console.log(resultData); 
          $('#allevent').append(resultData);
        },
        complete: function(data) {
          // Hide image container
          $("#loader").hide();
        }
      });

    });


    $('#nav-all-tab').click(function() {
      var id = 0;

      function isEmpty(el) {
        return !$.trim(el.html())
      }
      if (isEmpty($('#allevent'))) {
        $.ajax({
          type: 'POST',
          url: "https://www.tixtm.com/getnearbyevents/" + id,
